<%= form_tag sessions_path, :method => :post do %>
  <div class="row">
    <div class="small-12 columns">
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </div>
  </div>

  <div class="row">
    <div class="small-12 columns">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
  </div>
  <button>Login</button>
<% end %>
